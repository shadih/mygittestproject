	<topFilter name="Device_LAG_Suppression">
		<anyCondition>
			<allCondition  tag="Trigger,SubAlarm">
				<stringFilterStatement>
					<fieldName>originatingManagedEntity</fieldName>
					<operator>contains</operator>
					<fieldValue>DEVICE</fieldValue>
				</stringFilterStatement>
				<stringFilterStatement>
					<fieldName>component</fieldName>
					<operator>contains</operator>
					<fieldValue>PortLagId=</fieldValue>
				</stringFilterStatement>
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isEqual</operator>
					<fieldValue>50003/100/1</fieldValue>
				</stringFilterStatement>
			</allCondition>			
			<allCondition  tag="SubAlarm">
				<stringFilterStatement>
 					<fieldName>originatingManagedEntity</fieldName>
					<operator>contains</operator>
					<fieldValue>DEVICE</fieldValue>
				</stringFilterStatement>
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isEqual</operator>
					<fieldValue>50003/100/1</fieldValue>
				</stringFilterStatement>
				<stringFilterStatement>
					<fieldName>component</fieldName>
					<operator>doesNotContain</operator>
					<fieldValue>PortLagId=</fieldValue>
				</stringFilterStatement>				
			</allCondition>	
		</anyCondition>
	</topFilter>

	<!-- Juniper LAG linkdown is in Device level.  It is enriched here
	based on existing active Juniper PPORT linkdown -->
	<topFilter name="LAG_PPORT_Correlation">	<!-- tj -->
		<anyCondition>
			<!-- Not "Trigger,SubAlarm" so it won't be 'captured'
				by other group in "LAG_PPORT_Correlation" -->
			<allCondition  tag="Trigger">
				<stringFilterStatement>
					<fieldName>originatingManagedEntity</fieldName>
					<operator>contains</operator>
					<fieldValue>DEVICE</fieldValue>
				</stringFilterStatement>
				<stringFilterStatement>
					<!-- PortLagId is in component field
						not in text field, see 
						"Device_LAG_Suppression" -->
					<fieldName>component</fieldName>
					<operator>contains</operator>
					<fieldValue>PortLagId=</fieldValue>
				</stringFilterStatement>
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isEqual</operator>
					<fieldValue>50003/100/1</fieldValue>
				</stringFilterStatement>
			</allCondition>			
			<allCondition  tag="SubAlarm">
				<stringFilterStatement>
 					<fieldName>originatingManagedEntity</fieldName>
					<operator>contains</operator>
					<!-- below is PPORT, "Device_LAG_Suppression" is DEVICE -->
					<fieldValue>PPORT</fieldValue>
				</stringFilterStatement>
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isEqual</operator>
					<fieldValue>50003/100/1</fieldValue>
				</stringFilterStatement>
			</allCondition>	
		</anyCondition>
	</topFilter>

	<!-- Make Ciena Unreachable alarm as a secondary based on existing 
		active Juniper PPORT linkdown -->
	<topFilter name="Unreachable_PPORT_Correlation">	<!-- tj -->
		<anyCondition>
			<!-- Not "Trigger,SubAlarm" so it won't be 'captured'
			by other group in "Unreachable_PPORT_Correlation" -->
			<allCondition  tag="Trigger">
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isEqual</operator>
					<fieldValue>50002/100/55</fieldValue>
				</stringFilterStatement>
			</allCondition>			
			<allCondition  tag="SubAlarm">
				<stringFilterStatement>
 					<fieldName>originatingManagedEntity</fieldName>
					<operator>contains</operator>
					<fieldValue>PPORT</fieldValue>
				</stringFilterStatement>
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isEqual</operator>
					<fieldValue>50003/100/1</fieldValue>
				</stringFilterStatement>
			</allCondition>	
		</anyCondition>
	</topFilter>

