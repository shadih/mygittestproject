<?xml version="1.0" encoding="UTF-8" ?>
<filters xmlns="http://hp.com/uca/expert/filter">

<!--  CienaPdvp doesn't receive health trap
	<topFilter name="HealthTrap">
		<allCondition tag="Trigger">
			<stringFilterStatement>
				<fieldName>EventKey</fieldName>
				<operator>isListedIn</operator>
				<fieldValue>50002/100/58916873,50004/2/58916876</fieldValue>
			</stringFilterStatement>
		</allCondition>
	</topFilter>
-->

	<topFilter name="PtpProcess">
		<anyCondition>
			<allCondition tag="Trigger,SubAlarm">
				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isListedIn</operator>
					<fieldValue>50002/100/52,50001/100/61,50001/100/62,50001/100/63,50001/100/64,50001/100/65</fieldValue>
				</stringFilterStatement>
				<stringFilterStatement>
					<fieldName>vrf-name</fieldName>
					<operator>doesNotContain</operator>
					<fieldValue>FBS</fieldValue>
				</stringFilterStatement>
				<anyCondition>
					<stringFilterStatement>
						<fieldName>vrf-name</fieldName>
						<operator>contains</operator>
						<fieldValue>VPWS</fieldValue>
					</stringFilterStatement>
					<stringFilterStatement>
						<fieldName>vrf-name</fieldName>
						<operator>contains</operator>
						<fieldValue>L2CKT</fieldValue>
					</stringFilterStatement>
				</anyCondition>
			</allCondition>
		</anyCondition>
	</topFilter>

	<topFilter name="MptProcess">
		<anyCondition>
			<allCondition tag="Trigger,SubAlarm">
				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isListedIn</operator>
					<fieldValue>50002/100/52,50001/100/61,50001/100/62,50001/100/63,50001/100/64,50001/100/65</fieldValue>
				</stringFilterStatement>
				<stringFilterStatement>
					<fieldName>vrf-name</fieldName>
					<operator>contains</operator>
					<fieldValue>VPLS</fieldValue>
				</stringFilterStatement>
			</allCondition>
		</anyCondition>
	</topFilter>
	
</filters>
