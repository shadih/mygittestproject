<?xml version="1.0" encoding="UTF-8" ?>
<filters xmlns="http://hp.com/uca/expert/filter">
   <topFilter name="test">
		<allCondition>
			<stringFilterStatement>
				<fieldName>sourceIdentifier</fieldName>
				<operator>isNotEqual</operator>
				<fieldValue>NTD</fieldValue>
                <!--  Or another example of filter (for filtering on the BOX class)
                     <fieldValue>BOX .*</fieldValue>
                -->
			</stringFilterStatement>
	 		<stringFilterStatement>
				<fieldName>SuppressDevice</fieldName>
				<operator>isEqual</operator>
				<fieldValue>false</fieldValue> 
			</stringFilterStatement>
			<stringFilterStatement>
				<fieldName>SuppressPport</fieldName>
				<operator>isEqual</operator>
				<fieldValue>false</fieldValue>
			</stringFilterStatement>  
			<stringFilterStatement>
				<fieldName>SuppressLport</fieldName>
				<operator>isEqual</operator> 
				<fieldValue>false</fieldValue>
			</stringFilterStatement>  
		</allCondition>  
    </topFilter>  
</filters>
