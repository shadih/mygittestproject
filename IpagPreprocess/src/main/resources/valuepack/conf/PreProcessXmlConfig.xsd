<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns="http://config.pd.vp.expert.uca.hp.com/"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="2.0"
	targetNamespace="http://config.pd.vp.expert.uca.hp.com/" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified">

  <xs:element name="PreProcessPolicies">  
    <xs:complexType>
      <xs:sequence>
        <xs:element name="passThru">  
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:boolean" name="enablePublish"/>
              <xs:element name="eventNames">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="eventName" maxOccurs="unbounded" minOccurs="0"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
     <xs:element name="fileTargets">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="fileTarget" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:boolean" name="enableLogging"/>
                    <xs:element type="xs:string" name="fileName"/>
                    <xs:element name="eventNames">
                      <xs:complexType>
                        <xs:sequence>
                   			<xs:element type="xs:string" name="eventName" maxOccurs="unbounded" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element> 
     <xs:element name="cascadeTargets">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="cascadeTarget" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:boolean" name="enableCascading"/>
                    <xs:element type="xs:string" name="vpName"/>
                    <xs:element type="xs:string" name="scenarioName"/>
                    <xs:element type="xs:float" name="version"/> 
                    <xs:element name="eventNames">
                      <xs:complexType> 
                        <xs:sequence> 
                          <xs:element type="xs:string" name="eventName" maxOccurs="unbounded" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                </xs:complexType>
                <xs:unique name="unique-name">
			      <xs:selector xpath="cascadeTarget"/>    
			      <xs:field xpath="@name"/>          
			   </xs:unique>  
              </xs:element>   
            </xs:sequence>
          </xs:complexType>
        </xs:element>    
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>  

