	<!-- set it here so health traps can get to this Pdvp.
	it is sent by CienaExtendedLifeCycle.  it doesn't go to WM ==> its
	problem group implementation is default	-->
	<topFilter name="HealthTrap">
		<allCondition tag="Trigger">
			<stringFilterStatement>
				<fieldName>EventKey</fieldName>
				<operator>isListedIn</operator>
				<!-- not used
				 Ciena health trap is changed from
				  50002/100/58916873 to 50004/2/2
				 Infovista health trap is 50004/2/58916876
				-->
				<fieldValue>50004/2/2,50004/2/58916876</fieldValue>
			</stringFilterStatement>
		</allCondition>
	</topFilter>
	
<!-- below CienaUnreachableTrap is only needed if we don't want to insert ciena unreach. to WM and use 
		CienaUnreachableTrap.java (which is available in 2016.07 generic) which doesn't insert to WM and doesn't send it out
			
	<topFilter name="CienaUnreachableTrap">
		<allCondition tag="Trigger">
			<stringFilterStatement>
				<fieldName>EventKey</fieldName>
				<operator>isListedIn</operator>
				<fieldValue>50002/100/55</fieldValue>
			</stringFilterStatement>
		</allCondition>
	</topFilter> -->

	<topFilter name="CienaLD_Suppression">
		<anyCondition>
			<allCondition  tag="Trigger">
				<stringFilterStatement>
					<fieldName>originatingManagedEntity</fieldName>
					<operator>contains</operator>
					<fieldValue>PPORT</fieldValue>
				</stringFilterStatement>
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isListedIn</operator>
					<fieldValue>50002/100/21</fieldValue>
				</stringFilterStatement>
			</allCondition>			
			<allCondition  tag="SubAlarm">
				<stringFilterStatement>
					<fieldName>originatingManagedEntity</fieldName>
					<operator>contains</operator>
					<fieldValue>DEVICE</fieldValue>
				</stringFilterStatement>
 				<stringFilterStatement>
					<fieldName>EventKey</fieldName>
					<operator>isListedIn</operator>
					<fieldValue>50002/100/11,50002/100/14</fieldValue>
				</stringFilterStatement>
			</allCondition>			
		</anyCondition>
	</topFilter>

